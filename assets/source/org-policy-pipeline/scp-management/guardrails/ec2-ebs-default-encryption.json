[
  {
    "Sid": "EBSEncryption",
    "Effect": "Deny",
    "Action": [
      "ec2:DisableEbsEncryptionByDefault",
      "ec2:ModifyEbsDefaultKmsKeyId",
      "ec2:ResetEbsDefaultKmsKeyId"
    ],
    "Resource": [
      "*"
    ],
    "Condition": {
      "ArnNotLike": {
        "aws:PrincipalARN": [
          "arn:aws:iam::*:role/AWSAFTExecution",
          "arn:aws:iam::*:role/AWSControlTowerExecution"
        ]
      }
    }
  }
]