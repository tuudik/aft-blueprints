[
  {
    "Sid": "BlockBilling",
    "Effect": "Deny",
    "Action": [
      "aws-portal:ModifyAccount",
      "aws-portal:ModifyBilling",
      "aws-portal:ModifyPaymentMethods"
    ],
    "Resource": [
      "*"
    ],
    "Condition": {
      "ArnNotLike": {
        "aws:PrincipalARN": [
          "arn:aws:iam::*:role/AWSAFTExecution",
          "arn:aws:iam::*:role/AWSControlTowerExecution"
        ]
      }
    }
  }
]