[
  {
    "Sid": "DeletePasswordPolicy",
    "Effect": "Deny",
    "Action": [
      "iam:DeleteAccountPasswordPolicy",
      "iam:UpdateAccountPasswordPolicy"
    ],
    "Resource": [
      "*"
    ],
    "Condition": {
      "ArnNotLike": {
        "aws:PrincipalARN": [
          "arn:aws:iam::*:role/AWSAFTExecution",
          "arn:aws:iam::*:role/AWSControlTowerExecution"
        ]
      }
    }
  }
]